<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/margin_padding10"
    android:background="@drawable/publish_item_all_background"
    android:orientation="vertical"
    android:padding="@dimen/margin_padding5" >

    <TextView
        android:id="@+id/tv_project_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_padding5"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="AAAAAAAAAAAAA"
        android:textColor="@color/black"
        android:textSize="@dimen/textSize16" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1.0px"
        android:background="@color/full_line" />

    <TextView
        android:id="@+id/tv_project_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14" />

    <TextView
        android:id="@+id/tv_project_model"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14" />

    <TextView
        android:id="@+id/tv_project_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14" />

    <TextView
        android:id="@+id/tv_project_common"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_project_common01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_project_common02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_project_common03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_project_common04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_padding5"
        android:text="AAAAAAAA"
        android:textColor="@color/color666666"
        android:textSize="@dimen/textSize14"
        android:visibility="gone" />

</LinearLayout>