<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaeaea"
    android:orientation="vertical" >

    <com.xiangxun.xacity.view.TitleView
        android:id="@id/comment_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        tools:ignore="UselessParent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/margin_padding10"
            android:layout_marginBottom="@dimen/margin_padding15"
            android:background="@drawable/business_detai_background"
            android:orientation="vertical"
            android:padding="@dimen/margin_padding5" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目名称:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目编号:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="施工单位:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_constru_org"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="开始时间:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_start_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="结束时间:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_end_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="申请单位:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_apply_org"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="申请时间:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_apply_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="审批结束时间:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_apply_end"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/project_area"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_area"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/project_manage"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_manage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="联系电话:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_contact_tel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/official_man"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_official_man"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="道路名称:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_road_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="道路类型:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_road_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="道路风格:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_road_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="占用类型:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_occupy_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/occupy_w"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_occupy_w"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/occupy_z"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_occupy_z"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="项目类型:"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_padding10"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/project_remark"
                    android:textColor="@color/color_888888"
                    android:textSize="@dimen/textSize16" />

                <TextView
                    android:id="@+id/tv_project_remark"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/margin_padding8"
                    android:text=""
                    android:textColor="@color/color333333"
                    android:textSize="@dimen/textSize16" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>