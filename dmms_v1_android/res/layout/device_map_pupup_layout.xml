                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="15991624055"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="联系电话:"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />

            <TextView
                android:id="@+id/tv_tel_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="15991624055"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="产权单位:"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />

            <TextView
                android:id="@+id/tv_vehicle_org"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="西郊养护公司生产经营部"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="设备当前时间:"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />

            <TextView
                android:id="@+id/tv_location_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/margin_padding5"
                android:maxWidth="145.0dp"
                android:singleLine="true"
                android:text="2016-03-30 04:31:33"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize12" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>